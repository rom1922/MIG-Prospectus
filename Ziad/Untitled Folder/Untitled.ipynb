    "import numpy as np\n",
    "\n",
    "# 1. Définir la fonction d'activation (Sigmoïde)\n",
    "# Elle écrase les valeurs entre 0 et 1\n",
    "def sigmoid(x):\n",
    "    return 1 / (1 + np.exp(-x))\n",
    "\n",
    "# 2. Définir les données d'entrée\n",
    "# Votre exemple spécifique\n",
    "X = np.array([0.23, 0.82]) \n",
    "\n",
    "# 3. Initialisation des Poids (W) et des Biais (b)\n",
    "# On utilise des nombres aléatoires pour commencer (comme l'initialisation dans la montagne)\n",
    "\n",
    "# Couche 1 : connecte les 2 entrées aux 3 neurones cachés\n",
    "# Matrice de taille (2, 3)\n",
    "W1 = np.array([[0.1, 0.5], [0.4,0.3]])\n",
    "b1 = np.zeros(2)           \n",
    "\n",
    "# Couche 2 : connecte les 3 neurones cachés au 1 neurone de sortie\n",
    "# Matrice de taille (3, 1)\n",
    "W2 = np.array([0.2, 0.6])\n",
    "b2 = np.zeros(1)           \n",
    "\n",
    "# --- DÉBUT DU RÉSEAU (FORWARD PASS) ---\n",
    "\n",
    "# 4. Calcul de la Couche Cachée\n",
    "# Formule : z = x.W + b\n",
    "z1 = np.dot(X, W1) + b1    # Somme pondérée (Linéaire)\n",
    "a1 = sigmoid(z1)           # Activation (Non-linéaire)\n",
    "\n",
    "print(f\"Sortie de la couche cachée (a1) : {a1}\")\n",
    "\n",
    "# 5. Calcul de la Couche de Sortie\n",
    "# L'entrée de cette couche est 'a1' (le résultat de la précédente)\n",
    "z2 = np.dot(a1, W2) + b2   # Somme pondérée\n",
    "output = sigmoid(z2)       # Activation finale\n",
    "\n",
    "\n",
    "\n",
    "# --- RÉSULTAT ---\n",
    "\n",
    "print(f\"Prédiction finale du réseau : {output}\")"
   ]
  },
